{
  "info": {
    "name": "MySphere Backend API",
    "description": "Postman collection for all backend endpoints, organized by category and subcategory.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {"key": "base_url", "value": "http://127.0.0.1:8000"},
    {"key": "token", "value": ""}
  ],
  "item": [
    {
      "name": "Users",
      "item": [
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "url": {"raw": "{{base_url}}/api/v1/users/register/", "host": ["{{base_url}}"], "path": ["api", "v1", "users", "register"]},
            "body": {"mode": "raw", "raw": "{\n  \"username\": \"john_doe\",\n  \"email\": \"john@example.com\",\n  \"password\": \"your_password\"\n}"},
            "header": [{"key": "Content-Type", "value": "application/json"}]
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "url": {"raw": "{{base_url}}/api/v1/users/login/", "host": ["{{base_url}}"], "path": ["api", "v1", "users", "login"]},
            "body": {"mode": "raw", "raw": "{\n  \"username\": \"john_doe\",\n  \"password\": \"your_password\"\n}"},
            "header": [{"key": "Content-Type", "value": "application/json"}]
          }
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "url": {"raw": "{{base_url}}/api/v1/users/login/refresh/", "host": ["{{base_url}}"], "path": ["api", "v1", "users", "login", "refresh"]},
            "body": {"mode": "raw", "raw": "{\n  \"refresh\": \"<refresh_token>\"\n}"},
            "header": [{"key": "Content-Type", "value": "application/json"}]
          }
        }
      ]
    },
